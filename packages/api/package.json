{
  "name": "@omniapp-concept/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:check": "eslint --ext js,jsx,ts,tsx src",
    "lint:check:precommit": "npm run lint:check -- --max-warnings=0",
    "lint:fix": "npm run lint:check -- --fix",
    "ts:noWatch": "tsc --noEmit",
    "ts": "npm run ts:noWatch -- --watch",
    "dev": "nodemon -w .env -w src -r dotenv/config --transpile-only src/index.ts",
    "dev:inspect": "nodemon -w .env -w src --exec \"node --inspect -r dotenv/config -r ts-node/register src/index.ts\"",
    "dev:inspect-brk": "nodemon -w .env -w src --exec \"node --inspect-brk -r dotenv/config -r ts-node/register src/index.ts\"",
    "start": "node dist",
    "build": "rm -rf dist && tsc --noUnusedParameters --noUnusedLocals --sourceMap false --declarationMap false",
    "test": "../../node_modules/.bin/jest --runInBand -c=./jest.config.js",
    "husky:pre-commit:test": "npm run test",
    "migrate-mongo:create": "node -e \"fs.writeFileSync( require( 'path' ).resolve( __dirname, 'migrations', ( new Date().toISOString().replace(/\\-|\\T|\\:|\\.|\\Z/g, '').slice(0, -3) + '-RENAME-ME.js' ) ), '/** @type { Pick< import( \\'migrate-mongo\\' ), \\'up\\' > } */\\nconst migration = {\\n  up( db, client ) {\\n    /** */\\n  },\\n\\n  down( db, client ) {\\n    /** */\\n  },\\n};\\n\\nmodule.exports = migration;\\n' )\""
  },
  "dependencies": {
    "@illia-web-dev/logger": "0.0.2",
    "@illia-web-dev/types": "0.0.15",
    "@omniapp-concept/common": "1.0.0",
    "ajv": "8.12.0",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "bcrypt": "5.1.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.0",
    "jwt-decode": "3.1.2",
    "migrate-mongo": "10.0.0",
    "mongodb": "5.6.0",
    "ms": "2.1.3",
    "omit-deep": "0.3.0",
    "rfdc": "1.3.0",
    "winston": "3.9.0"
  },
  "devDependencies": {
    "@illia-web-dev/jest-tags": "0.0.1",
    "@jest/globals": "29.5.0",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/migrate-mongo": "10.0.0",
    "@types/ms": "0.7.31",
    "@types/node": "16.18.23",
    "@types/omit-deep": "0.3.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "nodemon": "2.0.21",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
